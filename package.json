{
  "name": "cru-merge-bot",
  "version": "1.0.0",
  "private": true,
  "description": "A Probot app for merging branches into staging",
  "author": "Spencer Oberstadt <soberstadt@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/soberstadt/merge-bot.git",
  "homepage": "https://github.com/soberstadt/merge-bot",
  "bugs": "https://github.com/soberstadt/merge-bot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./bot.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@probot/serverless-lambda": "^0.5.0",
    "probot": "^9.8.1"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nock": "^11.4.0",
    "nodemon": "^2.0.0",
    "serverless": "^1.44.1",
    "serverless-webpack": "^5.3.1",
    "smee-client": "^1.1.0",
    "standard": "^14.3.1",
    "webpack": "^4.2.0",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": ">= 8.3.0",
    "npm": ">= 4.0.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
